[core]
	excludesfile = $HOME/.gitignore
	editor = subl -w
[color]
	branch = auto
	diff = auto
	interactive = auto
	status = auto
	ui = auto
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
    whitespace = red reverse
[core]
    whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol
